CREATE TABLE `users`(`id` BIGINT PRIMARY KEY AUTO_INCREMENT,
`username` VARCHAR(30) NOT NULL,
`password` VARCHAR(26) NOT NULL,
`profile_picture` MEDIUMBLOB,
`last_login_time` DATETIME NULL,
`is_deleted` BOOLEAN  NOT NULL);
