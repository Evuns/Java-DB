package com.softuni.jsonProductStore.models.dtos;

import com.google.gson.annotations.Expose;

import java.util.Set;

public class ProductNameAndPriceViewDto {

    @Expose
    private int count;
    @Expose
    private Set<ProductNameAndPriceDto> products;

    public ProductNameAndPriceViewDto(int count, Set<ProductNameAndPriceDto> products) {
        this.count = count;
        this.products = products;
    }

    public int getCount() {
        return count;
    }

    public void setCount(int count) {
        this.count = count;
    }

    public Set<ProductNameAndPriceDto> getProducts() {
        return products;
    }

    public void setProducts(Set<ProductNameAndPriceDto> products) {
        this.products = products;
    }
}

