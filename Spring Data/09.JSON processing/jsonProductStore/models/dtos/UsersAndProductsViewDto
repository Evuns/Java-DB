package com.softuni.jsonProductStore.models.dtos;

import com.google.gson.annotations.Expose;

import java.util.List;

public class UsersAndProductsViewDto {

    @Expose
    private int usersCount;
    @Expose
    private List<UsersAndProductsDto> users;

    public UsersAndProductsViewDto(int usersCount, List<UsersAndProductsDto> users) {
        this.usersCount = usersCount;
        this.users = users;
    }

    public int getUsersCount() {
        return usersCount;
    }

    public void setUsersCount(int usersCount) {
        this.usersCount = usersCount;
    }

    public List<UsersAndProductsDto> getUsers() {
        return users;
    }

    public void setUsers(List<UsersAndProductsDto> users) {
        this.users = users;
    }
}
