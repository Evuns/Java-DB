package com.example.cardealer.models.dtos;

import com.example.cardealer.models.entities.Sale;

import java.util.Date;
import java.util.Set;

public class CustomersByBirthDateDto {
    private int Id;
    private String name;
    private Date birthDate;
    private boolean isYoungDriver;
    private Set<SaleForCustomerDto> sales;

    public CustomersByBirthDateDto() {
    }

    public int getId() {
        return Id;
    }

    public void setId(int id) {
        Id = id;
    }


    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public Date getBirthDate() {
        return birthDate;
    }

    public void setBirthDate(Date birthDate) {
        this.birthDate = birthDate;
    }

    public boolean getIsYoungDriver() {
        return isYoungDriver;
    }

    public void setYoungDriver(boolean youngDriver) {
        isYoungDriver = youngDriver;
    }

    public Set<SaleForCustomerDto> getSales() {
        return sales;
    }

    public void setSales(Set<SaleForCustomerDto> sales) {
        this.sales = sales;
    }
}
